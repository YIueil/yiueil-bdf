<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE root [
        <!ELEMENT root (config)*>
        <!ATTLIST root
                cache CDATA #REQUIRED>
        <!ELEMENT config (sql|params|filters|endSql)*>
        <!ATTLIST config
                id CDATA #REQUIRED
                description CDATA #IMPLIED>
        <!ELEMENT sql (#PCDATA)>
        <!ELEMENT params (param)*>
        <!ELEMENT param (#PCDATA)>
        <!ATTLIST param
                name CDATA #REQUIRED>
        <!ELEMENT filters (filter)*>
        <!ELEMENT filter (#PCDATA)>
        <!ATTLIST filter
                name CDATA #REQUIRED>
        <!ELEMENT endSql (#PCDATA)>
        ]>
<root cache="false">

</root>
