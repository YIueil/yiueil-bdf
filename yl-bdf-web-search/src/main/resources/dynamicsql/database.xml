<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE root PUBLIC "yl-dynamic-sql.xsd" "https://yiueil.github.io/static-repo/static/dtd/yl-dynamic-sql.dtd">
<root cache="true">
    <config id="getOrderList">
        <sql><![CDATA[
                select * from order where 1 = 1 and id in (:ids)
        ]]></sql>
        <params>
            <param name="id"/>
        </params>
        <filters>
            <filter name="name"><![CDATA[
                and name = :name
            ]]>
            </filter>
        </filters>
        <endSql><![CDATA[
            order by id desc
        ]]></endSql>
    </config>
</root>
